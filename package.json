{
  "name": "backend-test",
  "version": "1.0.0",
  "description": "La siguiente es una prueba para evaluar a los postulantes a programador **Back-end**.",
  "author": "Lenin Sanchez Aguilar",
  "private": true,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Megabite-Food/backend-test.git"
  },
  "bugs": {
    "url": "https://github.com/Megabite-Food/backend-test/issues"
  },
  "homepage": "https://github.com/Megabite-Food/backend-test#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.11",
    "@prisma/client": "^4.2.1",
    "@types/node": "^18.7.6",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
